@model MemberViewModel


<h2>Add new Member</h2>
<form asp-controller="Members" asp-action="AddMember" method="post">
    <label asp-for="FirstName">First Name: </label> <input type="text" asp-for="FirstName" />
    <label asp-for="LastName">Last Name: </label> <input type="text" asp-for="LastName" />
    <label asp-for="ActiveStatus">Active Status: </label> <input type="checkbox" asp-for="ActiveStatus" checked />

    <button type="submit">Create new member</button>
</form>